{
  "name": "my-turbo-repo",
  "version": "1.0.0",
  "packageManager": "pnpm@10.26.0",
  "description": "",
  "private": true,
  "engines": {
    "node": "20.19.x",
    "pnpm": "10.26.0"
  },
  "scripts": {
    "dev": "turbo run dev",
    "dev:vue": "turbo run dev --filter=vue",
    "dev:react": "turbo run dev --filter=react",
    "check": "turbo run check",
    "build": "turbo run build",
    "build:main": "turbo run build --filter=main...",
    "clean:mf": "rimraf .turbo apps/*/.__mf__temp apps/*/.turbo apps/*/dist apps/*/node_modules/__mf__virtual apps/*/node_modules/.vite apps/*/node_modules/.vite-temp",
    "clean:node_modules": "pnpm -r exec rimraf node_modules && rimraf node_modules",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@module-federation/vite": "^1.9.4",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "turbo": "^2.6.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vue-tsc": "^3.1.8"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
